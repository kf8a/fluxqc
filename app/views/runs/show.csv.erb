<% @run.incubations.each do |incubation | %>
  <% ['n2o','co2','ch4'].each do |compound| %>
    <% incubation.flux(compound).measurements.each do |c| %>
      <% next if c.excluded? %>
      <%= "#{@run.sampled_on}, #{@run.study}, #{incubation.treatment}, #{incubation.replicate}, #{incubation.lid.name},#{c.seconds}, #{c.ppm}, #{incubation.avg_height_cm}, #{compound}" %>
    <% end %>
  <% end %>
<% end %> 
