<% cache(run) do %>
<tr class="<%= recent(run) %>">
  <td><%= run.run_on %></td>
  <td><%= run.sampled_on %></td>
  <td><%= run.name %></td>
  <td><%= run.comment %></td>
  <td><%= run.study %></td>
  <td><%= "#{run.total_fluxes}" %></td>
  <td>
    <% if run.data_file.current_path %>
      <i class="fa fa-check"></i>
    <% end %>
    </td>
  <td><%= link_to 'qc', run ,:class =>'btn btn-success'%></td>
  <td><%= link_to 'edit', edit_run_path(run), :class=>'btn' %></td>
  <td><%= link_to 'download', run_url(run, :format => :csv), :class=>'btn' %></td>
  <td><%= link_to 'gc input', gcinput_run_url(run, :format=>:csv), :class=>'btn' %></td>
  <td><%= link_to 'destroy',  run, method: :delete, data: {confirm: "Are you sure you want to delete the #{run.name}?" } ,class: 'btn'%></td>
</tr>
<% end %>
